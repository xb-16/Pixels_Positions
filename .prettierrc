{
  "plugins": [
    "prettier-plugin-blade",
    "prettier-plugin-tailwindcss" 
  ],
  "overrides": [
    {
      "files": ["*.blade.php"],
      "options": {
        "parser": "blade"
      }
    }
  ],
  "printWidth": 120,
  "singleQuote": true,
  "semi": false,
  "tabWidth": 4, 
  "useTabs": false
}